<%- include('../partials/header.ejs') %>
<%- include('../partials/navbar.ejs') %>

    <div class="container w-25">
        <h1>Edit Cabang</h1>
        <% if (error) { %>
            <p style="color: red;">cannot add : <%= error %></p>
        <% } %>
        <form action='/restaurant/<%= data.RestaurantId %>/cabang/list/<%= data.id %>/edit' method='post'>
            <div class="form-group">
                <br>
                <label for = "">Cabang</label>
                <br>
                <input type="text" class="form-control" name="cabang" value="<%= data.cabang %>" > 
            </div>
            <div class="form-group">
                <label for = "">No.Telp</label>
                <br>
                <input type="text" class="form-control" name="no_telp" value="<%= data.no_telp %>"> 
            </div>
            <div class="form-group">
                <label for = "">Alamat</label>
                <br>
                <input type="text" class="form-control" name="alamat" value="<%= data.alamat %>"> 
            </div>
            <br>
            <br>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>

<%- include('../partials/footer.ejs') %>